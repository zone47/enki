{
  "manifest_version": 2,
  "name": "Enki - Wikidata Louvre Collections",
  "version": "0.5",
  "permissions": [
    "https://*/*",
    "http://*/*",
	"storage"
  ],
  "browser_specific_settings": {
	"gecko": {
		"id": "benoit@zone47.com",
		"strict_min_version": "74.0"
	}
  },
  "description": "Ajoute un bloc d'informations via Wikidata Ã  une notice du site Collections du Louvre - module non officiel",
  "icons": {
    "48": "icons/enki48.png"
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "content_scripts": [
    {
      "matches": ["https://collections.louvre.fr/*"],
      "js": ["wikilouvre.js"]
    }
  ]
}